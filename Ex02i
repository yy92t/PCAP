print(1 + 2 * 3)   # Multiply peforms first
print((1 + 2) * 3) # Forcing Add first
print()

print(4 + 2 > 3 * 1)   # Arithmatic peforms first
print(4 + (2 > 3) * 1) # Forcing Comparison first
print()

print(3 > 1 and 4 > 3)   # Comparison peforms first
print(3 > (1 and 4) > 3) # Forcing Logical first
print()

print(not True or True)   # not peforms first
print(not (True or True)) # Forcing or first
print()

print(not 1 + 2 * 3 > 8  and 3 > 4 + -5)
print(not ((1 + 2) * 3 > 8  and 3 > 4 + -5))
