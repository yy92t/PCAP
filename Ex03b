str1 = "Apple"
count = 0
for c in str1:
    print(c)
    if c == "p":
        count += 1
print(f"Number of p in str1 is : {count}")        
print()

sum = 0
for i in range(4):
    print(i)
    sum += i
print(f"Sum of all numbers upto {i} is : {sum}")
print()

for i in range(5, 11, 2):
    print(i)
else:
    print("All numbers iterated")
print()

print("Program ends") 
